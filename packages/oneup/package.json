{
	"name": "@jacob-ebey/oneup",
	"version": "0.0.5",
	"type": "module",
	"description": "IDK what this is yet.",
	"files": [
		"**/*"
	],
	"sideEffects": false,
	"types": "./index.d.ts",
	"main": "./index.js",
	"exports": {
		".": {
			"types": "./index.d.ts",
			"default": "./index.js"
		},
		"./entry.browser": {
			"types": "./entry.browser.d.ts",
			"default": "./entry.browser.js"
		}
	},
	"bin": {
		"oneup": "./cli.cjs"
	},
	"scripts": {
		"build": "repo-tools build --platform neutral && repo-tools build --nocheck --platform neutral --input entry.browser.ts --output entry.browser.js"
	},
	"devDependencies": {
		"oneup-repo-tools": "0.0.3"
	},
	"optionalDependencies": {
		"oneup-cli": "0.0.5",
		"oneup-node": "0.0.5",
		"oneup-react": "0.0.5"
	}
}
